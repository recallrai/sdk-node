{
	"name": "recallrai",
	"version": "0.4.0",
	"description": "Official Node.js SDK for RecallrAI - Revolutionary contextual memory system that enables AI assistants to form meaningful connections between conversations, just like human memory.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./errors": {
			"require": "./dist/errors/index.js",
			"import": "./dist/errors/index.js",
			"types": "./dist/errors/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsc",
		"lint": "eslint src --ext .ts",
		"format": "prettier --write src",
		"format:check": "prettier --check src"
	},
	"keywords": [
		"ai",
		"memory",
		"context",
		"llm",
		"mem0",
		"getzep",
		"zep"
	],
	"author": "RecallrAI",
	"license": "MIT",
	"homepage": "https://recallrai.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/recallrai/sdk-node"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"axios": "^1.6.0",
		"prettier": "^3.6.2",
		"uuid": "^9.0.1",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@types/node": "^20.8.6",
		"@types/uuid": "^9.0.5",
		"@typescript-eslint/eslint-plugin": "^6.8.0",
		"@typescript-eslint/parser": "^6.8.0",
		"eslint": "^8.51.0",
		"typescript": "^5.2.2"
	},
	"engines": {
		"node": ">=14.0.0"
	}
}
